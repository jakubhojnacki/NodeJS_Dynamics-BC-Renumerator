{
	"Init": {
		"prefix": "xini",
		"body": [
			"this.m${1/(.)(.*)/${1:/upcase}$2/} = $2.validate(p${1/(.)(.*)/${1:/upcase}$2/});"
		]
	},

	"ArrayInit": {
		"prefix": "xarrini",
		"body": [
			"this.m${1/(.)(.*)/${1:/upcase}$2/} = Array.validate(p${1/(.)(.*)/${1:/upcase}$2/});"
		]
	},

	"BooleanInit": {
		"prefix": "xbooini",
		"body": [
			"this.m${1/(.)(.*)/${1:/upcase}$2/} = Boolean.validate(p${1/(.)(.*)/${1:/upcase}$2/});"
		]
	},

	"IntegerInit": {
		"prefix": "xintini",
		"body": [
			"this.m${1/(.)(.*)/${1:/upcase}$2/} = Number.validateAsInteger(p${1/(.)(.*)/${1:/upcase}$2/});"
		]
	},

	"NumberInit": {
		"prefix": "xnumini",
		"body": [
			"this.m${1/(.)(.*)/${1:/upcase}$2/} = Number.validate(p${1/(.)(.*)/${1:/upcase}$2/});"
		]
	},

	"ObjectInit": {
		"prefix": "xobjini",
		"body": [
			"this.m${1/(.)(.*)/${1:/upcase}$2/} = Object.validate(p${1/(.)(.*)/${1:/upcase}$2/});"
		]
	},

	"StringInit": {
		"prefix": "xstrini",
		"body": [
			"this.m${1/(.)(.*)/${1:/upcase}$2/} = String.validate(p${1/(.)(.*)/${1:/upcase}$2/});"
		]
	},

	"Property": {
		"prefix": "xpro",
		"body": [
			"get $1() { return this.m${1/(.)(.*)/${1:/upcase}$2/}; }",
			"set $1(pValue) { this.m${1/(.)(.*)/${1:/upcase}$2/} = pValue; }"
		]
	},
	
	"GetProperty": {
		"prefix": "xgetpro",
		"body": [
			"get $1() { return this.m${1/(.)(.*)/${1:/upcase}$2/}; }"
		]
	},

	"StaticProperty": {
		"prefix": "xstapro",
		"body": [
			"static get $1() { return $2; }"			
		]
	},

	"Class": {
		"prefix": "xcla",
		"body": [
			"/**",
			" * @module \"$1\" class",
			" * @description $2",
			" * @version 0.0.1 ($CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE)",
			" */",
			"",
			"import \"../general/javaScript.js\";",
			"",
			"export default class $1 {",
			"    constructor() {",
			"    }",
			"}"
		]
	},

	"Import": {
		"prefix": "ximp",
		"body": [
			"import ${1/(.)(.*)/${1:/upcase}$2/} from \"../$2/${1/(.)(.*)/${1:/downcase}$2/}.js\";"
		]
	},

	"JavaScript": {
		"prefix": "xjs",
		"body": [
			"import \"../general/javaScript.js\";"
		]
	},

	"Module": {
		"prefix": "xmod",
		"body": [
			"/**",
			" * @module \"$1\" class",
			" * @description $2",
			" * @version 0.0.1 ($CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE)",
			" */"
		]
	},

	"Enum": {
		"prefix": "xenu",
		"body": [
			"/**",
			" * @module \"$1\" class",
			" * @description $2",
			" * @version 0.0.1 ($CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE)",
			" */",
			"",
			"import \"../general/javaScript.js\";",
			"import Enum from \"../general/enum.js\";",
			"import EnumItem from \"../general/enumItem.js\";",
			"",
			"export default class $1 {",
			"    static get one() { return \"one\"; }",
			"    static get two() { return \"two\"; }",
			"    static get three() { return \"three\"; }",
			"",
			"    static get values() { return [",
			"        new EnumItem($1.one),",
			"        new EnumItem($1.two),",
			"        new EnumItem($1.three)",
			"    ]; }",
			"",
			"    static parse(pText) {",
			"        return Enum.parse(pText, $1.values, $1.name);",
			"    }",
			"",
			"    static toString(pValue) {",
			"        return Enum.toString(pValue, $1.values, $1.name);",
			"    }",
			"}"
		]
	}
}